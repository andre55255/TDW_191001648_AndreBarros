{
    "name": "backend",
    "version": "1.0.0",
    "description": "A Api Bakers",
    "main": "server.js",
    "scripts": {
        "start": "nodemon",
        "build": "npx tsc",
        "knex:migrate:make": "knex --knexfile src/domain/knexfile.ts migrate:make",
        "knex:migrate:latest": "knex --knexfile src/domain/knexfile.ts migrate:latest",
        "knex:migrate:rollback": "knex --knexfile src/domain/knexfile.ts migrate:rollback"
    },
    "keywords": [
        "API",
        "Knex",
        "Node",
        "Typescript",
        "Swagger"
    ],
    "author": "Andr√© Luiz Barros",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.1",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^8.5.1",
        "knex": "^2.3.0",
        "mysql2": "^2.3.3",
        "swagger-ui-express": "^4.5.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.14",
        "@types/express-validator": "^3.0.0",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/knex": "^0.16.1",
        "@types/node": "^18.7.23",
        "@types/nodemon": "^1.19.2",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/winston": "^2.4.4",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
